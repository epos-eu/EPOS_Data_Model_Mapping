edm:Organization a owl:Class;
  owl:equivalentClass foaf:Organization ;
  owl:equivalentClass schema:Organization ;
.

edm:address a rdf:Property ;
    rdfs:domain edm:Organization ;
    rdfs:range  edm:Address ;
